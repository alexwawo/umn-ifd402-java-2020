      <RadioButton fx:id="radioDelete" layoutX="511.0" layoutY="65.0" mnemonicParsing="false" text="Delete" />
      <Button fx:id="btnAction" layoutX="375.0" layoutY="202.0" mnemonicParsing="false" onAction="#handleBtnAction" prefHeight="26.0" prefWidth="212.0" text="Button" />
