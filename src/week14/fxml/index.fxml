      <TableView fx:id="tableMahasiswa" layoutX="14.0" layoutY="56.0" prefHeight="317.0" prefWidth="345.0">
        <columns>
          <TableColumn fx:id="columnNim" prefWidth="82.0" text="NIM" />
          <TableColumn fx:id="columnNama" prefWidth="140.0" text="Nama" />
